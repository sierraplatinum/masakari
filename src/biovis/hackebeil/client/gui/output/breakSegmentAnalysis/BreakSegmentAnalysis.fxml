<Tab fx:id="tabBreakSegmentAnalysisRoot"
     text="Break Segment Analysis"
     closable="false"
     disable="true"
     xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
     fx:controller="biovis.hackebeil.client.gui.output.breakSegmentAnalysis.BreakSegmentAnalysisController"
>
    <content>
        <TabPane fx:id="tabs"
                 prefHeight="400.0" prefWidth="600.0"
                 tabMaxHeight="500.0" tabMaxWidth="500.0"
                 tabClosingPolicy="UNAVAILABLE"
        >
            <tabs>
                <Tab fx:id="heatmapCodeTab" closable="false" text="Heatmap Code">
                </Tab>
                <Tab fx:id="heatmapDistanceTab" closable="false" text="Heatmap Distance">
                </Tab>
                <Tab fx:id="distanceTab" closable="false" text="Barchart Distance">
                </Tab>
                <Tab fx:id="lengthTab" closable="false" text="Barchart Length">
                </Tab>
            </tabs>
        </TabPane>
    </content>
</Tab>